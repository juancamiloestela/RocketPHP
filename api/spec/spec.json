{
	"name": "Super Cool API",
	"baseUrl": "http://app.com/api",
	"version": "v1.0",
	"contexts": {
		"public": [],
		"logged": ["Contexts::is_logged"],
		"owns": ["Contexts::is_logged", "Contexts::is_owner", "Contexts::is_admin"]
	},
	"traits": {
		"paginated": {
			"description": "description of trait"
		}
	},
	"resources": {
		"User": {
			"description": "User entity",
			"properties": {
				"name": {
					"type": "string",
					"max_length": 30,
					"min_length": 3
				},
				"email": {
					"type": "email",
					"max_length": 30,
					"min_length": 5,
					"unique": true
				},
				"password": {
					"type": "string",
					"max_length": 60,
					"min_length": 60
				}
			},
			"endpoints":{
				"/users": {
					"public": {
						"GET": {
							"traits": ["paginated"],
							"returns": [{"id":"", "name":"", "email":""}]
						}
					}
				},
				"/users/{id}": {
					"public": {
						"GET": {
							"returns": {"id":"", "name":""}
						}
					}
				}
			}
		},
		"Blogs": {
			"description": "Blog entity",
			"properties": {
				"name": {
					"type": "string",
					"max_length": 30,
					"min_length": 3
				},
				"description": {
					"type": "string",
					"max_length": 400
				},
				"created": {
					"type": "datetime"
				},
				"updated": {
					"type": "datetime"
				}
			},
			"endpoints":{
				"/blogs": {
					"public": {
						"GET": {
							"traits": ["paginated"],
							"returns": [{}]
						},
						"POST": {
							"traits": ["TimeTracked"],
							"returns": {}
						}
					}
				},
				"/blogs/{id}": {
					"public": {
						"GET": {
							"traits": [],
							"returns": {}
						},
						"PUT": {
							"traits": ["TimeTracked"],
							"returns": {}
						}
					}
				}
			}
		}


	}
}